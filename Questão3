Var
   // Seção de Declarações das variáveis
   a, b, c: inteiro
   resultado: inteiro

Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   escreva("digite um número:")
   leia(a)
   limpatela
   escreva("digite um número:")
   leia(b)
   limpatela
   //
   se (a = b) entao
      resultado<-a+b
      //
      senao
         resultado<-(a*b)
      fimse
      //
      escreval("digite um número",a)
      escreval("digite um número",b)
      escreval("valor de C",resultado)



